{
	"name": "keyword-o-mat",
	"displayName": "Keyword-O-Mat",
	"publisher": "Turiiya",
	"description": "Easily cycle through commonly used keywords.",
	"version": "0.1.5",
	"license": "MIT",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"keyword",
		"rotate",
		"cycle",
		"toggle",
		"invert"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/tobealive/keyword-o-mat.git"
	},
	"icon": "images/keyword-o-mat__logo.png",
	"activationEvents": [
		"onCommand:keyword-o-mat.cycleForward",
		"onCommand:keyword-o-mat.cycleBackward"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "keyword-o-mat.cycleForward",
				"title": "K-O-M Cycle Forward"
			},
			{
				"command": "keyword-o-mat.cycleBackward",
				"title": "K-O-M Cycle Backward"
			}
		],
		"keybindings": [
			{
				"command": "keyword-o-mat.cycleForward",
				"key": "alt+a",
				"when": "editorTextFocus"
			},
			{
				"command": "keyword-o-mat.cycleBackward",
				"key": "alt+x",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-electron": "^2.1.3"
	}
}
